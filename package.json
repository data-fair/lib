{
  "name": "@data-fair/lib",
  "version": "0.6.1",
  "description": "A collection of javascript functions, components, types, etc to help work inside the data-fair stack.",
  "scripts": {
    "lint": "eslint . --fix",
    "build": "rm -rf ./types && tsc -p tsconfig.build.json",
    "build-types": "node src/types/build.js ./src/types/",
    "prepublish": "eslint . && npm test && npm run build-types && npm run build",
    "pre-test": "cd src/types/test && node ../build.js",
    "test": "npm run pre-test && node --test"
  },
  "files": [
    "src",
    "types",
    "LICENSE",
    "README.md"
  ],
  "type": "module",
  "exports": {
    "./express/session": {
      "default": "./src/express/session.js",
      "types": "./types/express/session.d.ts"
    },
    "./node/axios-auth": {
      "default": "./src/node/axio-auth.js",
      "types": "./types/node/axios-auth.d.ts"
    },
    "./node/axios": {
      "default": "./src/node/axios.js",
      "types": "./types/node/axios.d.ts"
    },
    "./node/http-agents": {
