{
  "name": "@data-fair/lib",
  "version": "0.6.1",
  "description": "A collection of javascript functions, components, types, etc to help work inside the data-fair stack.",
  "scripts": {
    "lint": "eslint . --fix",
    "build": "rm -rf ./types && tsc -p tsconfig.build.json",
    "build-types": "node src/types/build.js ./src/types/",
    "prepare": "npm run build-types && npm run build && husky install",
    "quality": "eslint . && npm test",
    "prepublishOnly": "npm run quality",
    "pre-test": "cd src/types/test && node ../build.js",
    "test": "npm run pre-test && node --test"
  },
  "files": [
    "src",
    "types",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "df-build-types": "./src/types/build.js"
  },
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./express/index.js": {
      "default": "./src/express/index.js",
      "types": "./types/express/index.d.ts"
    },
    "./express/session.js": {
      "default": "./src/express/session.js",
      "types": "./types/express/session.d.ts"
    },
    "./express/async-handler.js": {
      "default": "./src/express/async-handler.js",
      "types": "./types/express/async-handler.d.ts"
    },
    "./node/axios-auth.js": {
      "default": "./src/node/axio-auth.js",
      "types": "./types/node/axios-auth.d.ts"
    },
    "./node/axios.js": {
      "default": "./src/node/axios.js",
      "types": "./types/node/axios.d.ts"
    },
    "./node/http-agents.js": {
      "default": "./src/node/http-agents.js",
      "types": "./types/node/http-agents.d.ts"
    },
    "./node/prometheus.js": {
      "default": "./src/node/prometheus.js",
      "types": "./types/node/prometheus.d.ts"
    },
    "./node/ws.js": {
      "default": "./src/node/ws.js",
      "types": "./types/node/ws.d.ts"
    },
    "./node/mongo.js": {
      "default": "./src/node/mongo.js",
      "types": "./types/node/mongo.d.ts"
    },
    "./types/validation.js": {
      "default": "./src/types/validation.js",
      "types": "./types/types/validation.d.ts"
    },
    "./types/session-state.js": {
      "default": "./src/types/session-state/index.js",
      "types": "./types/types/session-state/index.d.ts"
    },
    "./types/identity.js": {
      "default": "./src/types/identity/index.js",
      "types": "./types/types/identity/index.d.ts"
    },
    "./vue/use-session.js": {
      "default": "./src/vue/use-session.js",
      "types": "./types/vue/use-session.d.ts"
    },
    "./vuetivy/personal-menu.vue": {
      "default": "./src/vuetify/personal-menu.vue"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/data-fair/lib.git"
  },
  "author": "Alban Mouton <alban.mouton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/data-fair/lib/issues"
  },
  "homepage": "https://github.com/data-fair/lib#readme",
